<p id="notice"><%= notice %></p>

<div class="container">
  
<center><strong>Listing Video Selectors</strong></center>

<table class="table table-bordered table-striped" id="sortable">
  <thead>
    <tr>
      <th>Default selection</th>
      <th>Track</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <tbody>
    <% @video_selectors.each do |video_selector| %>
      <tr>
        <td><%= video_selector.default_selection %></td>
        <td><%= video_selector.track %></td>
        <td><%= link_to 'Show', video_selector %></td>
        <td><%= link_to 'Edit', edit_video_selector_path(video_selector) %></td>
        <td><%= link_to 'Destroy', video_selector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video selector', new_video_selector_path %>

</div>