<p id="notice"><%= notice %></p>

<div class="container">
  
<center><strong>Listing Audio Selectors</strong></center>

<table class="table table-bordered table-striped" id="sortable">
  <thead>
    <tr>
      <th>Default selection</th>
      <th>Track</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>

    <% @audio_selectors.each do |audio_selector| %>
      <tr>
        <td><%= audio_selector.default_selection %></td>
        <td><%= audio_selector.track %></td>
        <td><%= link_to 'Show', audio_selector %></td>
        <td><%= link_to 'Edit', edit_audio_selector_path(audio_selector) %></td>
        <td><%= link_to 'Destroy', audio_selector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Audio selector', new_audio_selector_path %>

</div>